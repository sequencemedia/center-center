{
  "root": true,
  "extends": "standard",
  "env": {
    "es6": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": [
        "*.mts",
        "*.cts"
      ],
      "extends": "love",
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "project": "tsconfig.json"
      },
      "plugins": [
        "@typescript-eslint"
      ]
    }
  ]
}
